<div class="container">
  <div class="row">
    <div class="col">
      <h1>Survey Builder</h1>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <%= simple_form_for @survey do |f| %>
        <%= f.input :title %>
        <%= f.submit 'Save Survey', class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
